  "query": "\n            UPDATE configurations\n            SET\n                config_value = $2,\n                description = COALESCE($3, description),\n                updated_at = CURRENT_TIMESTAMP\n            WHERE config_key = $1\n            RETURNING\n                id,\n                config_key,\n                config_value,\n                description,\n                data_type as \"data_type!: ConfigDataType\",\n                is_encrypted as \"is_encrypted!: bool\",\n                is_required as \"is_required!: bool\",\n                created_at,\n                updated_at\n            ",
